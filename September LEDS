#include <FastLED.h>
#define NUM_LEDS 135
#define DATA_PIN 11

CRGB leds[NUM_LEDS];

voide set up(){
    FastLED,addLeds<ws21812B,DATA_PIN>(leds,NUM_LEDS);
}
void loop(){
    for (int i = 0;<NUM_LEDS; i++){
        leds[(NUM_LED-5)]=CRGB::Red
        leds[(NUM_LED-6)]=CRGB::Blue
        leds[(NUM_LED-7)]=CRGB::Green
        FastLED.show();
        delay(50);
        leds[(NUM_LED-8)]=CRGB::Black}
    for (int i=72, n<NUM_LEDS,n+=72){
        for (int i=0;i=n;i++){
            LEDS[i]=CRGB::Green;
            LEDS[i]=CRGB::Blue;
            LEDS[i]=CRGB::Red;
            FastLED.show();
            delay(10);
            LEDS[i]=CRGB::Yellow;}
        for (int i=0;i<n;i++){
            leds[n-i]=CRGB::White;
            FastLED.show();
            leds[n-i]=CRGB::Black}     
    }
}
